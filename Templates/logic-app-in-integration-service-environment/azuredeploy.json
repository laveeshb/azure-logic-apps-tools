{
	"$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"logicAppName": {
			"type": "string",
			"metadata": {
				"description": "The name of the logic app to create."
			}
		},
		"testUri": {
			"type": "string",
			"metadata": {
				"description": "A test URI"
			}
		},
		"integrationServiceEnvironmentResourceId": {
			"type": "string",
			"metadata": {
				"description": "Full resource Id of the integration service environment to create the logic app in."
			}
		},
		"location": {
			"type": "string",
			"defaultValue": "[resourceGroup().location]",
			"metadata": {
				"description": "Location of the integration service environment."
			}
		}
	},
	"variables": {},
	"resources": [{
		"type": "Microsoft.Logic/workflows",
		"apiVersion": "2019-05-01",
		"name": "[parameters('logicAppName')]",
		"location": "[parameters('location')]",
		"tags": {
			"displayName": "LogicApp"
		},
		"properties": {
			"integrationServiceEnvironment": {
				"id": "[parameters('integrationServiceEnvironmentResourceId')]"
			},
			"definition": {
				"$schema": "https://schema.management.azure.com/providers/Microsoft.Logic/schemas/2016-06-01/workflowdefinition.json#",
				"contentVersion": "1.0.0.0",
				"parameters": {
					"testUri": {
						"type": "string",
						"defaultValue": "[parameters('testUri')]"
					}
				},
				"triggers": {
					"recurrence": {
						"type": "recurrence",
						"recurrence": {
							"frequency": "Hour",
							"interval": 1
						}
					}
				},
				"actions": {
					"http": {
						"type": "Http",
						"inputs": {
							"method": "GET",
							"uri": "@parameters('testUri')"
						}
					}
				},
				"outputs": {}
			},
			"parameters": {}
		}
	}],
	"outputs": {}
}